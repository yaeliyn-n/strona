{
  "name": "strona-kroniki-elary",
  "version": "1.0.0",
  "description": "Strona internetowa dla serwera Discord Kroniki Elary",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:server": "nodemon server.js",
    "dev:client": "npm run dev --prefix client",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "build:client": "npm run build --prefix client",
    "heroku-postbuild": "npm run build:client",
    "postinstall": "cd client && npm install"
  },
  "keywords": [
    "discord",
    "anime",
    "manga",
    "kroniki elary"
  ],
  "author": "Arcadius",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",  
    "express": "^4.19.2", 
    "express-session": "^1.18.0", 
    "multer": "^1.4.5-lts.1", 
    "node-fetch": "^2.7.0", 
    "nodemailer": "^6.9.7", 
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
